  "repository": "diegoefe/tools",
  "contributors": [{
    "name": "Diego Florio", "email": "diegoefe@gmail.com"
  }],
  "dependencies": {
    "lodash": "3.10.1",
    "fs-extra": "0.24.0",
    "fs-promise": "0.3.1",
    "read-yaml-promise": "1.0.2",

    "expect-called": "0.4.0",
    "best-promise": "0.2.4"
  },
  "devDependencies": {
    "istanbul": "0.3.20",
    "mocha": "2.3.2",
    "expect.js": "0.3.1",
    "memoizee": "0.3.9",
    "moment": "2.10.6"
  },
  "main": "tools.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/"
  },
  "jshintConfig": {
    "asi": false,
    "curly": true,
    "forin": true
  },
  "codenautas": {
    "run-in": "server",
    "test-appveyor": true
  },
  "qa-control": {
    "package-version": "0.1.3",
    "run-in": "server",
    "stability": "extending",
    "test-appveyor": true,
    "type": "lib",
    "coverage": 90
  }
}
